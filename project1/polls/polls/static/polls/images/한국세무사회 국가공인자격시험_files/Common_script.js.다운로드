<!--// <span id=block1-5 으로 하고, onclick="clickshow(1-5)"으로 하면 전체가 다 열리는 소스임 //-->
	function clickshow(num) 
	{ 
		for (i=1;i<5;i++)  
		{ 
			menu=eval("document.all.block"+i+".style"); 
			//imgch=eval("document.bar"+i); 
			
			if (num==i)  
			{ 
				if (menu.display=="block")  
				{ 
					menu.display="none"; 
					//imgch.src="+.gif"; 
				} 
				else 
				{ 
					menu.display="block"; 
					//imgch.src="-.gif"; 
				} 
			} 
		} 
	}

	old=''
  
	function menu(name) 
	{
		submenu=eval("submenu_"+name+".style");
 		if(old!=submenu)	
 		{
      		if(old!='')	
      		{
        		old.display='none';
      		}
      		submenu.display='block';
      		old=submenu;
		}
		else	
		{
      		submenu.display='none';
      		old='';
		}
	}  
